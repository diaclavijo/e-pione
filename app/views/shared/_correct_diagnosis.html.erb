
<blockquote>
  <div class="row">
    <h3 class="text-muted">¿Esta de acuerdo con este diagnóstico?</h3>
  </div>
  <br>
  <div class="row">
		<div class="col-md-2 col-md-offset-3">
			<%= form_for @computer_diagnosis,
									 url: url do |f| %>
				<%= f.hidden_field :correct, value: 1 %>
				<%= f.submit 'Sí', class: 'btn btn-info btn-block' %>
			<% end %>
		</div>
    <div class="col-md-2">
			<%= form_for @computer_diagnosis,
									 url: url do |f| %>
				<%= f.hidden_field :correct, value: 0 %>
				<%= f.submit 'No', class: 'btn btn-default btn-block' %>
			<% end %>
    </div>
    <div class= "col-md-2" >
			<%= form_for @computer_diagnosis,
									 url: url do |f| %>
				<%= f.hidden_field :correct, value: 2 %>
				<%= f.submit 'Indeterminado', class: 'btn btn-default btn-grey btn-block' %>
			<% end %>
    </div>
  </div>
</blockquote>
