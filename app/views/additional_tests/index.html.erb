<!--URL patients/1/consultations/needed_tests-->

<% breadcrumb :consultation_additional_tests, @consultation %>

<% content_for :top_title do %>
   Tests adicionales  consulta <span class="text-muted"><%= @consultation.date %></span>
<% end %>
<% content_for :title do %>
    Realizar Tests
<% end %>



<%= render 'test_choice' %>

<!-- RESUMEN DE TESTS REALIZADOS EN ESTA CONSULTA-->
<div class="container"
<%= render partial: 'shared/tests_history',
					 locals: { tests:  @consultation.tests.order(created_at: :desc) } %>
<div class="row">
  <div class="col-md-1">
    <%link_type = "back" %>
    <%= render partial: 'shared/links',
               locals: { link_type: link_type } %>
  </div>
</div>


</div>
